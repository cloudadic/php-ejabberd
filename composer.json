{
    "name": "cloudadic/php-ejabberd",
    "description": "Ejabberd Client Library for chat apps",
    "type": "library",
    "authors": [
        {
            "name": "Prashant Bhardwaj",
            "email": "pkpbhardwaj729@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mcrypt": "*",
        "php": ">=5.6.0"
    },
    "require-dev": {
        "codeception/codeception": "2.2.*",
        "codeception/specify": "*",
        "codeception/verify": "*"
    },
    "autoload": {
        "psr-4": {
            "Cloudadic\\Ejabberd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cloudadic\\Ejabberd\\Tests\\": "tests/"
        }
    }
}
